{
	"name": "dataflowForData",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "testingdata",
						"type": "DatasetReference"
					},
					"name": "capitaltesting"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "datanewcsv",
						"type": "DatasetReference"
					},
					"name": "CapitalTestingDestination"
				}
			],
			"transformations": [
				{
					"name": "ModifyColumns1",
					"description": "Autogenerated by data preview actions"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> capitaltesting",
				"capitaltesting derive(Column_3 = upper(Column_3)) ~> ModifyColumns1",
				"ModifyColumns1 filter(lesserOrEqual(Column_2, '170')&& equals(Column_1,'45')) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Column_1 as string,",
				"          Column_2 as string",
				"     ),",
				"     partitionFileNames:['SortedData.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> CapitalTestingDestination"
			]
		}
	}
}